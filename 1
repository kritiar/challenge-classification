import numpy as np
import pandas as pd
from matplotlib import pyplot as plt
from sklearn.essemble import RandomForestClassifier
from sklearn.cross_validation import train_test_split
%matplotlib inline

data=pd.read_csv ("answers.csv")
data.head()
a= data.iloc[3,2:].values
a=a.reshape(28,28).astype ("unit8")
plt.imshow(a)
df_x= data.iloc[:,1:]
df_y= data.iloc[:,0]
x_test,y_train,x_train, y_test= train_test_split (df_x,df_y, test_size= 0.2, random_state=4)
y_train.head()
rf= RandomForestClassifier(n_estimators=100)
rf.fit(x_train, y_train)
pred=rf. predict(x_test)
pred

count

len(pred)

